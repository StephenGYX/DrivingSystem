<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.drivingsys.dao.DrivingSchoolManageMapper">


	<select id="queryCoachByMySchool" resultType="com.drivingsys.bean.Practise" parameterType="long">
	    select  * from  practise where  pdrivingid =#{pdrivingid}
	</select>
	<select id="queryCoachByMySchoolCount" resultType="long" parameterType="long">
	    select  count(*) from  practise where  pdrivingid =#{pdrivingid}
	</select>

</mapper>
	